{
  "name": "@asup/context-menu",
  "version": "1.5.2",
  "description": "REACT Typescript Context menu component",
  "author": "Paul Thomas <@PaulDThomas>",
  "private": false,
  "license": "MIT",
  "homepage": "https://github.com/PaulDThomas/context-menu#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulDThomas/context-menu.git"
  },
  "bugs": {
    "url": "https://github.com/PaulDThomas/context-menu/issues"
  },
  "keywords": [
    "react",
    "typescript",
    "context menu"
  ],
  "main": "dist/cjs/main.js",
  "module": "dist/main.js",
  "types": "dist/context-menu.d.ts",
  "files": [
    "dist"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "prepare": "husky",
    "start": "rmdir /q/s .\\.parcel-cache && parcel demo/index.html --dist-dir demo/dist",
    "test": "jest --collectCoverage=true",
    "test-watch": "jest --watch --collectCoverage=true --maxWorkers=4",
    "eslint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" \"demo/**/*.{js,jsx,ts,tsx}\"",
    "build": "rmdir /q/s .\\.parcel-cache && parcel build src/main.ts"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.17",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.28.0",
    "@parcel/packager-raw-url": "^2.7.0",
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.0",
    "@parcel/transformer-webmanifest": "^2.7.0",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.2",
    "@semantic-release/release-notes-generator": "^14.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.19",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.0.11",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-watch-typeahead": "^3.0.1",
    "lint-staged": "^16.1.0",
    "parcel": "^2.7.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^4.1.0",
    "process": "^0.11.10",
    "semantic-release": "^25.0.2",
    "ts-jest": "^29.4.5",
    "ts-jest-mock-import-meta": "^1.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "typescript-eslint": "^8.34.0",
    "typescript-plugin-css-modules": "^5.1.0"
  },
  "@parcel/transformer-css": {
    "cssModules": {
      "pattern": "aiw-[name]-[hash]-[local]",
      "dashedIndents": true
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
