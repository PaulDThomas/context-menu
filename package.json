{
  "name": "@asup/context-menu",
  "version": "1.3.7",
  "description": "REACT Typescript Context menu component",
  "author": "Paul Thomas <@PaulDThomas>",
  "private": false,
  "license": "MIT",
  "homepage": "https://github.com/PaulDThomas/context-menu#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulDThomas/context-menu.git"
  },
  "bugs": {
    "url": "https://github.com/PaulDThomas/context-menu/issues"
  },
  "keywords": [
    "react",
    "typescript",
    "context menu"
  ],
  "main": "dist/cjs/main.js",
  "module": "dist/main.js",
  "types": "dist/context-menu.d.ts",
  "files": [
    "dist"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "prepare": "husky",
    "start": "rmdir /q/s .\\.parcel-cache && parcel demo/index.html --dist-dir demo/dist",
    "test": "jest --maxWorkers=1",
    "test-watch": "jest --watch --collectCoverage=true --maxWorkers=4",
    "test-coverage": "jest --collectCoverage=true --maxWorkers=4",
    "build": "rmdir /q/s .\\.parcel-cache && parcel build src/main.ts"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.7.0",
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.0",
    "@parcel/transformer-webmanifest": "^2.7.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.2",
    "@types/node": "^20.11.19",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.3.1",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.2.2",
    "parcel": "^2.7.0",
    "prettier": "^3.2.5",
    "process": "^0.11.10",
    "ts-jest": "^29.0.3",
    "ts-jest-mock-import-meta": "^1.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "@parcel/transformer-css": {
    "cssModules": {
      "pattern": "aiw-[name]-[hash]-[local]",
      "dashedIndents": true
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
